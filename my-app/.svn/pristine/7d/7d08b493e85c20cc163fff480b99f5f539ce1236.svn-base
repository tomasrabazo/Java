package com.mycompany.app;

/**
 * Hello world!
 *
 */
public class App 
{
	private int value = 0;
	
	private String name;
	
    public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}

	public static void main( String[] args )
    {
    	App app = new App(); 
    	app.setValue(9);
        System.out.println( "Hello World! " + app.getValue());
    }
}
